{
  "properties": {
    "connectionReferences": {
      "shared_commondataserviceforapps": {
        "runtimeSource": "embedded",
        "connection": {
          "connectionReferenceLogicalName": "pu_sharedcommondataserviceforapps_debef"
        },
        "api": {
          "name": "shared_commondataserviceforapps"
        }
      },
      "shared_outlook": {
        "runtimeSource": "embedded",
        "connection": {
          "connectionReferenceLogicalName": "pu_sharedoutlook_225ff"
        },
        "api": {
          "name": "shared_outlook"
        }
      }
    },
    "definition": {
      "$schema": "https://schema.management.azure.com/providers/Microsoft.Logic/schemas/2016-06-01/workflowdefinition.json#",
      "contentVersion": "1.0.0.0",
      "parameters": {
        "$connections": {
          "defaultValue": {},
          "type": "Object"
        },
        "$authentication": {
          "defaultValue": {},
          "type": "SecureObject"
        }
      },
      "triggers": {
        "When_a_row_is_added,_modified_or_deleted": {
          "metadata": {
            "operationMetadataId": "fadeb487-76bc-4cc5-8d97-9aede4b12c6b"
          },
          "type": "OpenApiConnectionWebhook",
          "inputs": {
            "host": {
              "connectionName": "shared_commondataserviceforapps",
              "operationId": "SubscribeWebhookTrigger",
              "apiId": "/providers/Microsoft.PowerApps/apis/shared_commondataserviceforapps"
            },
            "parameters": {
              "subscriptionRequest/message": 1,
              "subscriptionRequest/entityname": "pu_loans",
              "subscriptionRequest/scope": 4
            },
            "authentication": "@parameters('$authentication')"
          }
        }
      },
      "actions": {
        "ContosoElementaryImage": {
          "runAfter": {
            "Get_book_details": [
              "Succeeded"
            ]
          },
          "metadata": {
            "operationMetadataId": "677b453f-5a1b-450d-9998-19626c10c7ae"
          },
          "type": "Compose",
          "inputs": "<img src=\"data:image/jpeg;base64,\" alt=\"Contoso Library Logo\" style=\"width:150px;height:150px;\" />",
          "description": "This is an encoded version of an image. We use this as the signature to our email."
        },
        "Get_bookcopy_details": {
          "runAfter": {},
          "metadata": {
            "operationMetadataId": "bd485a6f-93f0-424c-b389-92915bcc2954"
          },
          "type": "OpenApiConnection",
          "inputs": {
            "host": {
              "connectionName": "shared_commondataserviceforapps",
              "operationId": "GetItem",
              "apiId": "/providers/Microsoft.PowerApps/apis/shared_commondataserviceforapps"
            },
            "parameters": {
              "entityName": "pu_bookcopieses",
              "recordId": "@triggerOutputs()?['body/_pu_bookcopy_value']"
            },
            "authentication": "@parameters('$authentication')"
          }
        },
        "Get_book_details": {
          "runAfter": {
            "Get_bookcopy_details": [
              "Succeeded"
            ]
          },
          "metadata": {
            "operationMetadataId": "24b232cd-45b3-451c-9c19-d2bc9ed72ba9"
          },
          "type": "OpenApiConnection",
          "inputs": {
            "host": {
              "connectionName": "shared_commondataserviceforapps",
              "operationId": "GetItem",
              "apiId": "/providers/Microsoft.PowerApps/apis/shared_commondataserviceforapps"
            },
            "parameters": {
              "entityName": "pu_books",
              "recordId": "@outputs('Get_bookcopy_details')?['body/_pu_book_value']"
            },
            "authentication": "@parameters('$authentication')"
          }
        },
        "Condition": {
          "actions": {
            "Send_an_email_(V2)-_Include_Image": {
              "runAfter": {
                "HTML_book_image": [
                  "Succeeded"
                ]
              },
              "metadata": {
                "operationMetadataId": "1391bac0-3f13-4cdc-953b-991e7cc54c66"
              },
              "type": "OpenApiConnection",
              "inputs": {
                "host": {
                  "connectionName": "shared_outlook",
                  "operationId": "SendEmailV2",
                  "apiId": "/providers/Microsoft.PowerApps/apis/shared_outlook"
                },
                "parameters": {
                  "emailMessage/To": "@triggerOutputs()?['body/pu_studentemail']",
                  "emailMessage/Subject": "New loan taken out",
                  "emailMessage/Body": "<p class=\"editor-paragraph\">@{triggerOutputs()?['body/pu_studentfullname']}, you have taken out @{outputs('Get_book_details')?['body/pu_bookname']} on loan.</p><p class=\"editor-paragraph\">@{outputs('HTML_book_image')}<br><br>The book is due back on@{formatDateTime(triggerOutputs()?['body/pu_datedue'],'ddd dd MMM yyyy')}<br><br>Many thanks the Contoso Library Team<br><br>@{outputs('ContosoElementaryImage')}</p>",
                  "emailMessage/Importance": "Normal"
                },
                "authentication": "@parameters('$authentication')"
              }
            },
            "HTML_book_image": {
              "runAfter": {},
              "metadata": {
                "operationMetadataId": "7cf49742-99ed-45f2-8d8c-c936f77b4b22"
              },
              "type": "Compose",
              "inputs": "<img src=\"data:image/jpeg;base64, @{outputs('Get_book_details')?['body/pu_bookimage']}\"/>"
            }
          },
          "runAfter": {
            "ContosoElementaryImage": [
              "Succeeded"
            ]
          },
          "else": {
            "actions": {
              "Send_an_email_(V2)-_No_Image": {
                "runAfter": {},
                "metadata": {
                  "operationMetadataId": "9fe6ec43-09d1-4e22-a7cd-7541e276b8c5"
                },
                "type": "OpenApiConnection",
                "inputs": {
                  "host": {
                    "connectionName": "shared_outlook",
                    "operationId": "SendEmailV2",
                    "apiId": "/providers/Microsoft.PowerApps/apis/shared_outlook"
                  },
                  "parameters": {
                    "emailMessage/To": "@triggerOutputs()?['body/pu_studentemail']",
                    "emailMessage/Subject": "New loan taken out - no book image",
                    "emailMessage/Body": "<p class=\"editor-paragraph\">@{triggerOutputs()?['body/pu_studentfullname']}, you have taken out @{outputs('Get_book_details')?['body/pu_bookname']} on loan.<br><br>The book is due back on@{formatDateTime(triggerOutputs()?['body/pu_datedue'],'ddd dd MMM yyyy')}<br><br>Many thanks the Contoso Library Team<br><br>@{outputs('ContosoElementaryImage')}</p>",
                    "emailMessage/Importance": "Normal"
                  },
                  "authentication": "@parameters('$authentication')"
                }
              }
            }
          },
          "expression": {
            "and": [
              {
                "not": {
                  "equals": [
                    "@outputs('Get_book_details')?['body/pu_bookimage']",
                    "@null"
                  ]
                }
              }
            ]
          },
          "metadata": {
            "operationMetadataId": "abe591cd-1bc4-4d90-8a6b-488b02a79047"
          },
          "type": "If"
        }
      },
      "outputs": {}
    },
    "templateName": ""
  },
  "schemaVersion": "1.0.0.0"
}